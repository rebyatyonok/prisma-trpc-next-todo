{
	"extends": [
		"next/core-web-vitals",
		"airbnb-typescript"
	],
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"@typescript-eslint/no-unused-vars": [
			"error",
			{
				"varsIgnorePattern": "^_.+$",
				"argsIgnorePattern": "^_.+$"
			}
		],
		"@typescript-eslint/indent": [
			"off"
		]
	}
}
